{
  "name": "WikiAPPP",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "permissions": ["activeTab" ],
  "content_scripts": [
    {
	
      "matches": ["<all_urls>"],
		"js": ["contentScript.js"],
      "run_at": "document_end",
      "all_frames": true
    }]
}
