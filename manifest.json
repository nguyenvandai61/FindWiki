{
  "name": "WikiAPPP",
  "version": "1.0",
  "description": "Look up Wiki with double click!",
  "manifest_version": 2,
  "permissions": ["activeTab", "storage"],
   "browser_action": {
        "default_popup": "popup.html"
    },
  "content_scripts": [
    {
	
      "matches": ["<all_urls>"],
		"js": ["contentScript.js"],
      "run_at": "document_end",
      "all_frames": true
    }]
}
